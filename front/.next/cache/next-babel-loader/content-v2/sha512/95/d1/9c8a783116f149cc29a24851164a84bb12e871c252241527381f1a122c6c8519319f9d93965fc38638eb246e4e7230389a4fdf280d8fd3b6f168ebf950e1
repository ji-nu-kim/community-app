TypesUser.CHANGE_COMMUNITY_OF_ME:\n        if (draft.me) {\n          var communityIndex = draft.me.Communities.findIndex(function (community) {\n            return community.id === action.data.communityId;\n          });\n          draft.me.Communities[communityIndex].profilePhoto = action.data.profilePhoto;\n        }\n\n        break;\n\n      default:\n        break;\n    }\n  });\n};\n\nexport default reducer;","map":{"version":3,"sources":["C:/Users/wlsdn/Desktop/frontend/react/next/community/front/reducers/user.ts"],"names":["actionTypesUser","produce","initialState","imagePath","me","userInfo","logInLoading","logInDone","logInError","logOutLoading","logOutDone","logOutError","uploadImageLoading","uploadImageDone","uploadImageError","signUpLoading","signUpDone","signUpError","leaveLoading","leaveDone","leaveError","loadMyInfoLoading","loadMyInfoDone","loadMyInfoError","changeProfileLoading","changeProfileDone","changeProfileError","changeCountryLoading","changeCountryDone","changeCountryError","checkNotificationLoading","checkNotificationDone","checkNotificationError","removeNotificationLoading","removeNotificationDone","removeNotificationError","reducer","state","action","draft","type","LOG_IN_REQUEST","LOG_IN_SUCCESS","LOG_IN_ERROR","error","LOG_OUT_REQUEST","LOG_OUT_SUCCESS","LOG_OUT_ERROR","LOAD_MY_INFO_REQUEST","LOAD_MY_INFO_SUCCESS","data","LOAD_MY_INFO_ERROR","UPLOAD_IMAGE_REQUEST","UPLOAD_IMAGE_SUCCESS","UPLOAD_IMAGE_ERROR","REMOVE_IMAGE","SIGN_UP_REQUEST","SIGN_UP_SUCCESS","SIGN_UP_ERROR","LEAVE_REQUEST","LEAVE_SUCCESS","LEAVE_ERROR","CHANGE_PROFILE_REQUEST","CHANGE_PROFILE_SUCCESS","CHANGE_PROFILE_ERROR","CHANGE_COUNTRY_REQUEST","CHANGE_COUNTRY_SUCCESS","country","CHANGE_COUNTRY_ERROR","CHECK_NOTIFICATION_REQUEST","CHECK_NOTIFICATION_SUCCESS","Notices","CHECK_NOTIFICATION_ERROR","REMOVE_NOTIFICATION_REQUEST","REMOVE_NOTIFICATION_SUCCESS","filter","notice","id","notificationId","REMOVE_NOTIFICATION_ERROR","JOIN_MEET_OF_ME","ADD_MEET_OF_ME","Meets","u